  window.addEventListener("load", ()=> {
    setTimeout(()=> {
      let dots = document.querySelectorAll(".section_gallery20 .w-slider-dot");
      let slides = document.querySelectorAll(".gallery20_slide.w-slide");
    }, 1000);
  })
  
  function getVariantIdFromUrl() {
    const searchParams = new URLSearchParams(window.location.search);
    return searchParams.get('variant');
  }
  
  const variantId = getVariantIdFromUrl();
  console.log(variantId);
  
  function handleVariantChange() {
    const variantId = getVariantIdFromUrl();
    console.log('Selected variant:', variantId);
    const inputs = document.querySelectorAll('[data-variant-id]');


    let dots = document.querySelectorAll(".section_gallery20 .w-slider-dot");
    let slides = document.querySelectorAll(".gallery20_slide.w-slide");
    const varMobileId = document.querySelectorAll('[data-variant-id-mobile]');
    
    const varLoopId = document.querySelectorAll('[data-loop-id]');
    for(let i=0;i<dots.length; i++){
      console.log(i, varMobileId[i]);
      for(let j=0;j<varLoopId.length;j++){
        let varID = varMobileId[j];
        if(varMobileId[i]===varMobileId[j] && variantId === varID.getAttribute('data-variant-id-mobile')){
          console.log(i);
          dots[varLoopId[j].getAttribute('data-loop-id')].click();
        }
      }
    }
    
    inputs.forEach(function(input) {
      const inputVariantId = input.dataset.variantId;
      const img = input.value;
      if (inputVariantId === variantId && img) {
        img.src = input.value;
        const mainImage = document.querySelector('.product-header5_main-image');
        document.querySelector('.product-header5_main-image').src=img;
      }
    });
  }
  
  document.addEventListener('change', function(event) {
    const target = event.target;
    if (target && target.name === 'id') {
      setTimeout(handleVariantChange, 100);
    }
  });

</script>
