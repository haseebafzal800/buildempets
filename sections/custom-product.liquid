                        {
                          "items": [],
                          "group": ""
                        }
                      </script>
                    </a>
                  </div>
                      {% endunless %}
                  {% endfor %}
                </div>
                 <!-- -->
                {% if product.images.size > 5 %}
                  <button class="loadmore_btn" data-load="more">Load More</button>
                  {% endif %}
              </div>
            </div>
            <div class="product-header5_product-details-2">
              <div class="margin-bottom margin-small">
                {% comment %}<div class="product-header5_breadcrumb">
                  <a href="{{ shop.url }}" class="breadcrumb-link w-inline-block">
                    <div class="text-block-26">Home</div>
                  </a>
                  <div class="breadcrumb-divider w-embed">
                    <svg width="16" height="16" viewbox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path d="M6 3L11 8L6 13" stroke="CurrentColor" stroke-width="1.5"></path>
                    </svg>
                  </div>
                  {% if collection.url %}
                  <a href="{{ collection.url }}" class="breadcrumb-link w-inline-block">
                    <div class="text-block-25">{{ collection.title }}</div>
                  </a>
                  <div class="breadcrumb-divider w-embed">
                    <svg width="16" height="16" viewbox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path d="M6 3L11 8L6 13" stroke="CurrentColor" stroke-width="1.5"></path>
                    </svg>
                  </div>
                    {% endif %}
                  <a href="{{ product.url }}" class="breadcrumb-link w-inline-block">
                    <div class="text-block-24">{{ product.title }}</div>
                  </a>
                </div>{% endcomment %}
                <div class="mobile_item">
                  {% for block in section.blocks %}
                  {% if block.type == '@app' %}
                  <div class="ddd-wrap-2-6">
                   {% render block %}
                   </div>
                    {% endif %}
                  {% endfor %}
                </div>
                <div class="product-header5_heading-wrapper _2">
                  {%- assign product_form_id = 'product-form-' | append: section.id -%}
                  <h1 class="heading-style-h3">{{ product.title }}</h1>
                  <div class="heading-style-h4">
<!--                   <div class="no-js-hidden" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                    {%- render 'price',
                      product: product,
                      use_variant: true,
                      show_badges: true,
                      price_class: 'price--large'
                    -%}  
                  </div> -->
                    <div class="product-detail__price product-price" data-price-wrapper>
                      <span class="{% if current_variant.compare_at_price > current_variant.price %}product-price__reduced{% endif %}" data-product-price data-product-detail-price>
                        <span class="theme-money large-title">
                          {%- if settings.product_currency_code_enabled -%}
                            {{- current_variant.price | money_with_currency -}}
                          {%- else -%}
                            {{- current_variant.price | money -}}
                          {%- endif -%}
                        </span>
                      </span>

                      {%- if product.compare_at_price_max > product.price -%}
                        <span class="visually-hidden" data-compare-text>{{ 'products.product.regular_price' | t }}</span>
                        <span class="tiny-title" data-compare-price>
                          {%- if current_variant.compare_at_price > current_variant.price -%}
                            <span class="product-price__compare theme-money">{{ current_variant.compare_at_price | money }}</span>
                          {%- endif -%}
                        </span>
                      {%- endif -%}

                      {% render 'unit-price', variant: current_variant %}
                    </div>
                </div>
              </div>
