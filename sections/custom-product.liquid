                  {% for image in product.images limit:5 %}
                    {% unless forloop.first %}
                  <div class="product-header5_item">
                    <a href="#" class="product-header5_lightbox-link w-inline-block w-lightbox">
                      <div class="product-header5_image-wrapper">
