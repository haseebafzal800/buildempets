  <body
    id="{{ page_title | handle }}"
    class="
      template-{{ template | split: '.' | first }}
      {%- if template.suffix %} template-suffix-{{ template.suffix }}{% endif -%}
      {%- if settings.swatch_enabled_productpage %} swatch-method-{{ settings.swatch_method }}{% endif -%}
    "
  >
    <a class="in-page-link visually-hidden skip-link" href="#MainContent">
      {{- 'general.accessibility.skip_to_content' | t -}}
    </a>

    {% section 'header' %}

    {% unless template == 'product.em-dogs' %}
      <div class="container breadcrumbs-container">
        <div class="page-width">
          {% render 'breadcrumbs', show_next_prev: true %}
        </div>
      </div>
    {% endunless %}

    {% render 'productwiz-rio-scp-main', product: product, shop: shop %}

    <main id="MainContent">
      {%- assign accent_alpha = settings.accent_color | color_extract: 'alpha' -%}
      {%- if template contains 'index' -%}
        {%- if settings.color_body_background != settings.accent_color and accent_alpha > 0 -%}
          <span class="accent-background"></span>
        {%- endif -%}
        {% section 'banner' %}
      {%- endif -%}
      {{ content_for_layout }}
    </main>

    {% section 'footer' %}
    {% section 'popup' %}

    <a href="#" class="focus-tint" aria-hidden="true" aria-label="{{ 'general.accessibility_labels.close' | t }}"></a>

    {%- if template contains 'product' -%}
      {% render 'photoswipe' %}
    {%- endif -%}

    <script defer src="{{ 'vendor.min.js' | asset_url }}"></script>
    <script defer src="{{ 'theme.min.js' | asset_url }}"></script>

    {% render 'supercart-over' %}

    <!-- Google Tag Manager (noscript) -->
    <noscript
      ><iframe
        src="https://www.googletagmanager.com/ns.html?id=GTM-WX6358F"
        height="0"
        width="0"
        style="display:none;visibility:hidden"
      ></iframe
    ></noscript>
    <!-- End Google Tag Manager (noscript) -->
    {% include 'cjpod' %}
    <!-- Gem_Page_Footer_Script -->
    {% render 'gem-app-footer-scripts', GEM_FOOTER_SCRIPT: GEM_FOOTER_SCRIPT %}
    <!-- End_Gem_Page_Footer_Script -->

    {% comment %}
