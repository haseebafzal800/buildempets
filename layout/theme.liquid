      <script src="{{ 'webflow.min.js' | asset_url }}" type="text/javascript"></script>
      {% endif %}
    {% endcomment %}

    <script>
        $(document).on('click','.triggerButton',function(e){
          e.preventDefault();
          window.dataLayer = window.dataLayer || [];
          dataLayer.push({ ecommerce: null });
          dataLayer.push({
          event: "begin_checkout",
          });
          setTimeout(function(){
            $('.jsAccept').click();
          }, 100);
        });
      $(document).on('click','.jsPopAccept',function(e){
          e.preventDefault();

          var hrefLink = $(this).attr('href');

          window.dataLayer = window.dataLayer || [];
          dataLayer.push({ ecommerce: null });
          dataLayer.push({
          event: "begin_checkout",
          });
          setTimeout(function(){
           window.location.href = hrefLink;
          }, 100);
        });
          $(document).on('click','.supercart-checkout',function(e){
          e.preventDefault();
            window.dataLayer = window.dataLayer || [];
            dataLayer.push({ ecommerce: null });
            dataLayer.push({
            event: "begin_checkout",
            });
            setTimeout(function(){
             window.location.href = '/checkout';
            }, 100);
        });
    </script>
  </body>
</html>
